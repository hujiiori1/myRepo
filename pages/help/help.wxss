  display: flex;
  flex-direction: column;
  align-content: flex-start;
  justify-content: flex-start;
  box-sizing: border-box;
  flex-wrap: wrap;
  background-color: #eeeeee
} 

.content_image{
  width:118rpx;
  height: 118rpx;
  
}
.item{
  width:100vw;
  height:150rpx;
  background-color:white;
